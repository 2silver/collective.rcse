<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="document_actions" meta_type="CMF Action Category">
  <object name="ics_view" meta_type="CMF Action" i18n:domain="plone">
   <property name="title">title_add_to_ical</property>
   <property name="description" i18n:translate=""></property>
   <property name="url_expr">string:$object_url/ics_view</property>
   <property name="icon_expr"></property>
   <property name="available_expr">python:context.portal_type=="collective.rcse.event"</property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="cioppino_twothumbs_like" meta_type="CMF Action" i18n:domain="cioppino.twothumbs">
   <property name="title">Like</property>
   <property name="description">I like this (click again to undo)</property>
   <property name="url_expr">string:$object_url/@@cioppino_twothumbs_like</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="cioppino_twothumbs_dislike" meta_type="CMF Action" i18n:domain="cioppino.twothumbs">
   <property name="title">Ugh</property>
   <property name="description">I dislike this (click again to undo)</property>
   <property name="url_expr">string:$object_url/@@cioppino_twothumbs_dislike</property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
 <object name="user" meta_type="CMF Action Category">
 <!-- desactivate some plone base user action -->
  <object name="dashboard"><property name="visible">False</property></object>
  <object name="preferences"><property name="visible">False</property></object>
 </object>
</object>
