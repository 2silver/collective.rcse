<div id="modal-viewlet" i18n:domain="collective.rcse">
    <div class="modal fade" id="content-modal" tabindex="-1" role="dialog"
        aria-labelledby="content modal" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h4 class="modal-title">Modal title</h4>
                </div>
                <div class="modal-body">
                     <div class="rcse_tile_wrapper">
                       <a class="rcse_tile">test</a>
                     </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default"
                        data-dismiss="modal" i18n:translate="">Close</button>
                </div>
            </div>
        </div>
    </div>
    <div class="modal fade" id="img-modal" tabindex="-1" role="dialog"
