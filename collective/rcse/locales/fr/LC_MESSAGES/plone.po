msgid "Member makes content private"
msgstr "Le membre rend le contenu privé"

msgid "Member makes content private but open to request access"
msgstr "Le membre rend le contenu privé mais accessible au demande d'accès"

#: ./profiles/worfklows/collective_rcse_group_workflow/definition.xml
msgid "Member makes content visible to other internal users"
msgstr "Le membre rend le contenu visible à tous les utilisateurs connectés."

#: ./profiles/worfklows/collective_rcse_group_workflow/definition.xml
msgid "Moderated"
msgstr "Modéré"

#: ./profiles/worfklows/collective_rcse_group_workflow/definition.xml
