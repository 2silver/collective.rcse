<div class="rcse_tile_wrapper" i18n:domain="collective.rcse">
    <div class="rcsetile panel panel-default">
        <div class="panel-heading">
            <div class="media vcard byline">
                <a class="pull-left" tal:attributes="href view/url">
                    <img class="media-object img-thumbnail avatar"
                        tal:attributes="src string:${view/photo}/thumb" />
                </a>
                <div class="media-body">
                    <h4 class="media-heading">
                        <a class="fn url" tal:attributes="href view/url" tal:content="view/fullname"></a>
                    </h4>
                    <p i18n:translate=""><span i18n:name="function" tal:replace="view/function">Function</span> in <span i18n:name="company" tal:replace="view/company">Company</span></p>
                    <div class="bio" tal:condition="view/bio" tal:content="structure view/bio/output"></div>
                </div>
            </div>
        </div>
    </div>
</div>
