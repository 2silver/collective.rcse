<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="collective.rcse">

<body>
<div metal:fill-slot="main">
  <h1 i18n:translate="">Manage users</h1>
  <div tal:repeat="user view/getUsersForms">
    <div>
      <a tal:attributes="href user/user/getURL"
	 tal:content="user/user/Title"></a>
      <tal:with-form-as-view define="view nocall:user/form">
	<metal:block use-macro="user/form/@@ploneform-macros/titlelessform" />
      </tal:with-form-as-view>
    </div>
  </div>
</div>
</body>
</html>
