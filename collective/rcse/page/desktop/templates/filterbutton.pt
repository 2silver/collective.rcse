<html tal:omit-tag="" i18n:domain="collective.rcse">
<a data-toggle="modal" href="#rcsefiltersform" class="btn btn-default btn-lg"
   title="Open filter form" i18n:attributes="title">
    <span class="icon-stack">
        <i class="icon-check-empty icon-stack-base"></i>
        <i class="icon-filter"></i>
    </span>
    <span i18n:translate="">Filter</span>
</a>
<div class="modal fade" id="rcsefiltersform" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
          <h4 class="modal-title" i18n:translate="">Filter form</h4>
        </div>
        <div class="modal-body" tal:content="structure view/form_instance"></div>
      </div>
    </div>
</div>
</html>
