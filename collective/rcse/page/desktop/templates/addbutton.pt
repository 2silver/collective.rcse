<html tal:omit-tag="" i18n:domain="collective.rcse">
<a data-toggle="modal" href="#rcseaddform" class="btn btn-warning btn-lg"
   title="Open add form" i18n:attributes="title">
  <i class="icon-plus"></i>
  <span i18n:translate="">Add</span>
</a>
<div class="modal fade" id="rcseaddform" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog">
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
          <h4 class="modal-title" i18n:translate="">Add form</h4>
        </div>
        <div class="modal-body" tal:content="structure view/form_instance"></div>
        <div class="modal-footer">
          <button type="button" class="btn btn-default" data-dismiss="modal" i18n:translate="">Close</button>
          <button type="button" class="btn btn-primary" i18n:translate="">Add</button>
        </div>
      </div>
    </div>
</div>
</html>
