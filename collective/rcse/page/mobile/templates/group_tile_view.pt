<div class="rcsetile" tal:attributes="id view/tileid" i18n:domain="collective.rcse">
  <div class="vcard" tal:define="creator_info context/creator_memberinfo">
    <img class="photo" tal:attributes="src creator_info/photo"/>
    <a rel="author" class="fn url"
       tal:attributes="href creator_info/url"
       tal:content="creator_info/formated_name">John</a><br/>
    <a href=""  data-inline="true" tal:attributes="href view/group_url" tal:content="view/group_title">group</a><br/>
    <span tal:condition="view/effective_date" tal:content="view/effective_date"></span>
  </div>
  <div class="tilecontent" tal:content="structure view/get_content"></div>
  <div class="actions" data-role="controlgroup" data-type="horizontal">
    <a href="#" data-icon="eye-open" class="ui-icon-nodisc" data-iconshadow="false" data-role="button" data-inline="true" i18n:translate="" data-theme="b" tal:attributes="href string:${context/absolute_url}">Read</a>
  </div>
</div>