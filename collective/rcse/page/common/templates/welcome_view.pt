<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      lang="en"
      metal:use-macro="here/main_template/macros/master"
      i18n:domain="collective.rcse">

<body>
  <div metal:fill-slot="main" tal:define="auth_memberinfo context/auth_memberinfo">
    <tal:main-macro metal:define-macro="main">

    <h1 i18n:translate="Welcome">My space</h1>

    <div class="rcseblock">
        <h2 i18n:translate="">My profile</h2>
        <div tal:replace="structure context/auth_vcard_view"/>
          <tal:tile tal:repeat="item view/get_content">
            <a class="rcse_tile" tal:attributes="href item; id repeat/item/index"></a>
          </tal:tile>

    </div>


    </tal:main-macro>
  </div>
</body>
</html>